{
  "name": "eslint-plugin-agentlint",
  "version": "0.1.0",
  "description": "ESLint plugin that makes your UI accessible to AI agents",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "docs"
  ],
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "ai",
    "agent",
    "accessibility",
    "a11y",
    "automation",
    "react",
    "jsx",
    "lint",
    "agentlint"
  ],
  "author": "Ratik",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ratikkoka/eslint-plugin-agentlint"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@typescript-eslint/rule-tester": "^8.0.0",
    "@typescript-eslint/utils": "^8.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.0.0",
    "eslint-plugin-eslint-plugin": "^6.0.0",
    "eslint-plugin-n": "^17.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0",
    "typescript-eslint": "^8.55.0",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^8.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
